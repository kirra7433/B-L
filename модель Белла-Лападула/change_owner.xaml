<Window x:Class="модель_Белла_Лападула.change_owner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:модель_Белла_Лападула"
        mc:Ignorable="d"
        Title="change_owner" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock TextWrapping="Wrap" Text="Введите имя субъекта"/>
            <TextBox x:Name="TB_subj" Height="23" TextWrapping="Wrap" Text=""/>
            <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="Введите имя объекта"/>
            <TextBox x:Name="TB_obj" Height="23" TextWrapping="Wrap" Text=""/>
            <Button x:Name="btnChange" Content="Изменить" Click="btnChange_Click"/>
        </StackPanel>
    </Grid>
</Window>
